    "type": "module",
    "scripts": {
        "build": "vite-node src/index.ts",
        "build:ext": "vsce package --no-dependencies",
        "lint": "prettier --write ./src/**/*.ts && eslint --fix"
    },
    "dependencies": {
        "@types/node": "^24.7.0",
        "@types/vscode": "^1.104.0",
        "@typescript-eslint/eslint-plugin": "^8.46.0",
        "@typescript-eslint/parser": "^8.46.0",
        "@vscode/vsce": "^3.6.2",
        "colord": "^2.9.3",
        "eslint": "^9.37.0",
        "eslint-plugin-perfectionist": "^4.15.1",
        "ovsx": "^0.10.6",
        "prettier": "^3.6.2",
        "typescript": "^5.9.3",
        "vite-node": "^3.2.4"
