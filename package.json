{
    "name": "eggsthetic",
    "displayName": "Eggsthetic",
    "description": "A pack of pitch-black color themes for your OLED'd pleasure.",
    "version": "0.1.0",
    "publisher": "friedegg",
    "repository": {
        "type": "git",
        "url": "https://github.com/friedegg/eggsthetic.git"
    },
    "homepage": "https://github.com/friedegg/eggsthetic#readme",
    "author": {
        "name": "friedegg",
        "url": "https://github.com/friedegg"
    },
    "keywords": [
        "dark",
        "black",
        "contrast",
        "minimal",
        "pure",
        "oled"
    ],
    "icon": "images/icon.png",
    "galleryBanner": {
        "color": "#000000",
        "theme": "dark"
    },
