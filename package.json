            },
            {
                "label": "Eggsthetic Theme: Blood Moon",
                "uiTheme": "vs-dark",
                "path": "./themes/eggsthetic-theme-bloodMoon.json"
            },
            {
                "label": "Eggsthetic Theme: Campbell",
                "uiTheme": "vs-dark",
                "path": "./themes/eggsthetic-theme-campbell.json"
            },
            {
                "label": "Eggsthetic Theme: Chicago95",
                "uiTheme": "vs-dark",
                "path": "./themes/eggsthetic-theme-chicago95.json"
            },
            {
                "label": "Eggsthetic Theme: Hyper",
                "uiTheme": "vs-dark",
                "path": "./themes/eggsthetic-theme-hyper.json"
            },
            {
                "label": "Eggsthetic Theme: Moonfly",
                "uiTheme": "vs-dark",
                "path": "./themes/eggsthetic-theme-moonfly.json"
            },
            {
                "label": "Eggsthetic Theme: Styrokai",
                "uiTheme": "vs-dark",
                "path": "./themes/eggsthetic-theme-styrokai.json"
